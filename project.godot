; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Clonecraft"
config/name_localized={
"en_US": "Clonecraft"
}
run/main_scene="res://init.tscn"
config/project_settings_override="user://settings.godot"
config/features=PackedStringArray("4.2", "Double Precision")
config/icon="res://icon.png"

[editor]

naming/node_name_casing=1
naming/scene_name_casing=1
export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/clonecraft_documentation/plugin.cfg")

[gameplay]

video/fov=50.0
video/use_vsync=true
video/framerate_cap=0
input/invert_mouse_x=false
input/invert_mouse_y=false
input/mouse_x_sensitivity=0.0
input/mouse_y_sensitivity=0.0
debug/show_updates=false
ui/auto_scale=false
ui/scale=1.0
physics/terminal_velocity=-60.0
debug/log_to_chat=false
ui/auto_scale_shown=false
mods/api_url="https://ce-studio.github.io/api/v1/clonecraft/modstore.json"

[gui]

theme/custom="res://textures/uitheme.tres"

[importer_defaults]

texture={
"detect_3d/compress_to": 0
}
oggvorbisstr={
"loop": false
}
2d_array_texture={
"compress/mode": 0
}
3d_texture={
"compress/mode": 0
}

